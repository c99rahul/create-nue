<!--
  Library of server-side components used by layout.nue

  See: scripts/render.js
-->

<div @name="logo">
  <a :href="href">
    <img class="logo" :src="img_src" />
  </a>
</div>

<figure @name="image-block">
  <img :src="image_src" alt="{caption}" />
  <figcaption>{caption}</figcaption>
</figure>
