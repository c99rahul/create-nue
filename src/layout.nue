<html>
  <meta charset="utf-8" />
  <meta name="generator" content="Nue (nuejs.org)" />
  <meta name="date.updated" content="{ timestamp.toISOString() }" />
  <meta name="viewport" content="width=device-width" />
  <link rel="shortcut icon" href="{ favicon }" />
  <title>{ title }</title>
  <style>
    { primary_css }
  </style>

  <logo img_src="img/logo.svg" href="//w3bits.com" />

  <counter />

  <image-block
    image_src="/img/sample.jpg"
    caption="This example demonstrates a universal Nue component (Image from Unsplash)"
  />

  <!-- setup all client-side/reactive components  -->
  <script type="module" src="setup.js"></script>

  <!-- secondary CSS -->
  <link
    :for="href in secondary_css"
    :href="href"
    rel="stylesheet"
    fetchpriority="low"
  />
</html>
